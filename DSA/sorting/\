#include<bits/stdc++.h>

using namespace std;

int partition(vector<int> &arr, int l, int r){
	int i{l-1}, j{r+1};
	do{
		i++;
	}while(arr[i] < arr[l]);
	do{
		j--;
	}while(arr[j] > arr[l]);
	if(arr[i] >= arr[j]) return j;
	swap(arr[i], arr[j]);
}

void sort(vector<int> &arr, int l, int r){
	if(l < r){
		int p =partition(arr, l, r);
		sort(arr, l, p);
		sort(arr, p+1, r);
	}
}

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	vector<int> arr {10, 5, 30, 12};
	sort(arr, 0, 3);
	
	cout << arr[2] << '\n';	

	return 0;
}
